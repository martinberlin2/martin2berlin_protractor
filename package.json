{
  "name": "martin2protractor",
  "version": "1.0.0",
  "description": "a typescript example",
  "author": "",
  "license": "MIT",
  "scripts": {
    "install": "npm install @types/node --save-dev",
    "debug": "node --inspect-brk ./node_modules/protractor/bin/protractor conf.js",
